#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
int main()
{
	const int num = 10;//通过const修饰后，num中的值则不允许被改动。
	int* p = &num;
	*p = 20;//但此时*p是可以改动num中的值的。


	const int num = 10;//通过const修饰后，num中的值则不允许被改动。
	const int* p = &num;//将指针变量p用const修饰时
    *p = 20;//*p就无法改动num中的值了。
	//const 放在指针变量*的左边时，修饰的是*p，即不能通过*p来改变num中的值。

	int* const p = &num;//const 放在指针变量*右边时，修饰的是指针变量p本身，p不能别改变了。
	*p = 20;
	int n = 100;
	p = &n;
	printf("%d\n", num);
	return 0;
}
