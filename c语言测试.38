#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
struct Stu
{
	//成员变量
	char name[20];
	int age;
	char id[20];
};
int main()
{
	int a = 10;
	//使用struct Stu这个类型创建一个对象s1，并初始化。
	struct  Stu s1 = { "张三",20 ,"2019010305" };
  //函数调用().成员名
	printf("%s\n", s1.name);
	printf("%d\n", s1.age);
	printf("%s\n", s1.id);
	
	struct Stu* ps = &s1;
  //结构体变量.成员名
	printf("%s\n", (*ps).name);
	printf("%d\n", (*ps).age);
	printf("%s\n", (*ps).id);
  
	//结构体指针->
	printf("%s\n", ps->name);
	printf("%d\n", ps->age);
	printf("%s\n", ps->id);
	return 0;
}
